<name>sys_widgets_20f65a3fd8e47100ff80e65e18e39f94</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_widgets"&gt;&lt;sys_widgets action="INSERT_OR_UPDATE"&gt;&lt;active&gt;true&lt;/active&gt;&lt;name&gt;Homepage Controls&lt;/name&gt;&lt;order&gt;100&lt;/order&gt;&lt;renderer/&gt;&lt;renderer_type&gt;javascript&lt;/renderer_type&gt;&lt;roles/&gt;&lt;script&gt;&lt;![CDATA[function sections() {
	var homeControls = new Object();
	var pageControl = new GlideRecord('u_homepage_controls');
	pageControl.addQuery('u_active', true);
	pageControl.query();
	while (pageControl.next()) {
		homeControls[pageControl.u_name] = {'type' : pageControl.u_long_name.toString().replace("homepage_control_", "")}
	}
	return homeControls;
}

function render() {
	var type = renderer.getPreferences().get("type");
	var gf = new GlideForm(renderer.getGC(), "homepage_control_" + type, 0);
	gf.setDirect(true);
	gf.setRenderProperties(renderer.getRenderProperties());
	return gf.getRenderedPage();
}

function getEditLink() {
	return "sys_ui_page.do?sysparm_query=name=homepage_control_" + renderer.getPreferences().get("type");
}]]&gt;&lt;/script&gt;&lt;sys_created_by&gt;admin_SHARE&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2014-11-29 20:26:11&lt;/sys_created_on&gt;&lt;sys_id&gt;20f65a3fd8e47100ff80e65e18e39f94&lt;/sys_id&gt;&lt;sys_mod_count&gt;10&lt;/sys_mod_count&gt;&lt;sys_updated_by&gt;admin_SHARE&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2014-12-02 15:24:03&lt;/sys_updated_on&gt;&lt;/sys_widgets&gt;&lt;sys_app_file action="INSERT_OR_UPDATE"&gt;&lt;customer_update&gt;true&lt;/customer_update&gt;&lt;publish_override/&gt;&lt;replace_on_upgrade&gt;false&lt;/replace_on_upgrade&gt;&lt;restore/&gt;&lt;sys_app/&gt;&lt;sys_code&gt;!!N!7/&lt;/sys_code&gt;&lt;sys_created_by&gt;admin_SHARE&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2014-11-29 20:26:11&lt;/sys_created_on&gt;&lt;sys_id&gt;60f65a3fd8e47100ff80e65e18e39f94&lt;/sys_id&gt;&lt;sys_mod_count&gt;0&lt;/sys_mod_count&gt;&lt;sys_name&gt;Homepage Controls&lt;/sys_name&gt;&lt;sys_parent/&gt;&lt;sys_path&gt;!!N!7/&lt;/sys_path&gt;&lt;sys_policy/&gt;&lt;sys_source_deleted&gt;false&lt;/sys_source_deleted&gt;&lt;sys_source_id&gt;20f65a3fd8e47100ff80e65e18e39f94&lt;/sys_source_id&gt;&lt;sys_source_table&gt;sys_widgets&lt;/sys_source_table&gt;&lt;sys_type/&gt;&lt;sys_update_name&gt;sys_widgets_20f65a3fd8e47100ff80e65e18e39f94&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;admin_SHARE&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2014-12-02 15:24:03&lt;/sys_updated_on&gt;&lt;/sys_app_file&gt;&lt;/record_update&gt;</payload>
