<name>sys_ui_page_9270783ed834b100ff80e65e18e39fcc</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_ui_page"&gt;&lt;sys_ui_page action="INSERT_OR_UPDATE"&gt;&lt;category&gt;homepages&lt;/category&gt;&lt;client_script&gt;&lt;![CDATA[setSelected();

function changeSelected(select) {
	setPreference('homepage_control_team_members', getSelectValue(select), selectionChanged);
}

function getSelectValue(select) {
	var index = select.selectedIndex;
	return select.options[index].value;
}

function selectionChanged() {
	homeRefresh();
}

function setSelected() {
	if (!g_selectedOption)
		return;
	
	setSelectedValue('homepage_choice', g_selectedOption);
}

function setSelectedValue(name, value) {
	var e = gel(name);
	for (var i = 0; i != e.options.length; i++) {
		if (e.options[i].value == value) {
			e.options[i].selected = true;
		}
	}
}]]&gt;&lt;/client_script&gt;&lt;description&gt;Control Gauge for Dynamic Homepages&lt;/description&gt;&lt;html&gt;&lt;![CDATA[&lt;table width="100%"&gt;
&lt;tr&gt;
   &lt;td nowrap="true" align="center"&gt;
      &lt;label for="homepage_choice"&gt;Team Member: &lt;/label&gt;
      &lt;select name="homepage_choice" id="homepage_choice" onchange="changeSelected(this)"&gt;
         &lt;g2:evaluate&gt;
            var pageControl = new GlideRecord('u_homepage_controls');
            pageControl.addQuery('u_active', true);
            pageControl.addQuery('u_long_name', 'homepage_control_team_members');
            pageControl.query();
            if (pageControl.next()) {
               gs.getSession().setStrictQuery(true);
               var rec = new GlideRecord(pageControl.u_table);
               rec.addEncodedQuery(pageControl.u_condition);
               rec.query();
               var recList = [];
               while(rec.next()) {
                  key = rec.getDisplayValue(pageControl.u_choice_value) + '|' + rec.getValue(pageControl.u_choice_value);
                  recList.push(key);
               }
               var arrayUtil = new ArrayUtil();
               recList = arrayUtil.unique(recList);
               recList.sort();
            }
         &lt;/g2:evaluate&gt;
         &lt;j2:forEach var="jvar_choice" items="$[recList]"&gt;
            &lt;g2:evaluate jelly="true"&gt;
               var recString = jelly.jvar_choice.toString();
               var recDisplay = recString.substring(0,recString.indexOf("|"));
               var recValue = recString.substring(recString.indexOf("|")+1);
            &lt;/g2:evaluate&gt;
            &lt;option value="$[recValue]"&gt;$[recDisplay]&lt;/option&gt;
         &lt;/j2:forEach&gt;
      &lt;/select&gt;
      &lt;script language="javascript" eval="true"&gt;
         g_selectedOption = "${jvar_session.getUser().getPreference('homepage_control_team_members')}";
      &lt;/script&gt;
   &lt;/td&gt;
&lt;/tr&gt;
